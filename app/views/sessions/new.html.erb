<% flash.each do |name, msg| %>
  <div class="custom-alert">
    <%= msg %>
  </div>
<% end %>

<h3> Your premier destination for renting planes to take you where you want to go! </h3>

<br>

<%= form_tag sessions_path do %> 
  <%= label_tag "Name" %>
  <%= text_field_tag :name %>
  <br>
  <%= label_tag "Password" %>
  <%= password_field_tag :password %>
  <br>
  <div class="button">
    <a><%= submit_tag "Log in" %></a>
  </div>
<% end %>
